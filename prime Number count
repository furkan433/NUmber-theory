#include <bits/stdc++.h>
using namespace std;
void chaker( int n){
    int cont=0;
    for(int i=2;i*i<=n;i++){
       if(n%i==0){
           cont++;
           while(n%i==0){
               n/=i;
           }
       }
       if(1<n){
          cont++; 
       }
    }
    cout<<cont<<endl;
}

int main() {
    int a;
    cin>>a;
    while(a--){
        int n;
        cin>>n;
        chaker(n);
    }
    
    return 0;
}
